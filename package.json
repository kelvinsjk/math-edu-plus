{
  "name": "math-edu-plus",
  "version": "0.0.1",
  "description": "extras for the math-edu package",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "docs": "typedoc --sort instance-first --sort source-order",
    "pretest": "npm run build",
    "test": "jest --config jestconfig.json --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .gitignore",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelvinsjk/math-edu-plus.git"
  },
  "keywords": [
    "math"
  ],
  "author": "Kelvin Soh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kelvinsjk/math-edu-plus/issues"
  },
  "homepage": "https://github.com/kelvinsjk/math-edu-plus#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rollup": "^2.53.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.4",
    "typedoc-plugin-markdown": "^3.10.3",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib"
  ]
}
